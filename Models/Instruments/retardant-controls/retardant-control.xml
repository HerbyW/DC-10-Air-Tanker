<?xml version="1.0" encoding="UTF-8" ?>

<!-- McDonnell Douglas DC-10 -->
<!-- Retardant Controls by HerbyW -->

<PropertyList>

	<path>retardant-controls.ac</path>
    
    
    <!-- Quantity Selector 1-3 -->
<animation>
  <type>rotate</type>
  <object-name>turn11</object-name>
  <property>/controls/retardant[0]/dump-quantity</property>
	<interpolation>
	  <entry><ind> 0</ind><dep>   0</dep></entry>
	  <entry><ind> 1</ind><dep> -45</dep></entry>
	  <entry><ind> 2</ind><dep> -90</dep></entry>
	  <entry><ind> 3</ind><dep>-135</dep></entry>
	  <entry><ind> 4</ind><dep>-180</dep></entry>
	</interpolation>
  <center>
    <x-m> 0.02867</x-m>
    <y-m> -0.02685</y-m>
    <z-m> 0.07394</z-m>
  </center>
  <axis>
    <x>1</x>
    <y>0</y>
    <z>0</z>
  </axis>
</animation>

<animation>
  <type>pick</type>
  <object-name>turn11</object-name>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>property-adjust</command>
      <property>/controls/retardant[0]/dump-quantity</property>
      <step>1</step>
      <min>0</min>
      <max>4</max>
      <wrap>false</wrap>
    </binding>	
  </action>
  <action>
    <button>1</button>
    <repeatable>false</repeatable>
    <binding>
      <command>property-adjust</command>
      <property>/controls/retardant[0]/dump-quantity</property>
      <step>-1</step>
      <min>0</min>
      <max>4</max>
      <wrap>false</wrap>
    </binding>
  </action>  
</animation>

